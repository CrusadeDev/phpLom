{
  "name": "crusade-dev/php-lom",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "doctrine/annotations": "^1.13",
    "illuminate/collections": "^8.46",
    "nikic/php-parser": "^4.10",
    "symfony/finder": "5.3.*",
    "symfony/filesystem": "^5.3"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Crusade\\PhpLom\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Crusade\\PhpLom\\Tests\\": "tests/"
    }
  }
}
