{
  "name": "crusade-dev/php-lom",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "doctrine/annotations": "1.1.*",
    "illuminate/collections": "8.*",
    "nikic/php-parser": "4.*",
    "symfony/finder": "5.*",
    "symfony/filesystem": "5.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Crusade\\PhpLom\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Crusade\\PhpLom\\Tests\\": "tests/"
    }
  }
}
